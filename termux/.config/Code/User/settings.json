{
  "editor.guides.bracketPairs": true,
  "files.autoSave": "afterDelay",
  "editor.fontSize": 22,
  "git.ignoreMissingGitWarning": true,
  "window.commandCenter": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.find.seedSearchStringFromSelection": "selection",
  "diffEditor.diffAlgorithm": "legacy",
  "diffEditor.experimental.showMoves": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.minimap.enabled": false,
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.preferredDarkColorTheme": "Everforest Dark",
  "workbench.preferredHighContrastColorTheme": "Everforest Dark",
  "workbench.preferredHighContrastLightColorTheme": "Everforest Dark",
  "workbench.preferredLightColorTheme": "Everforest Dark",
  "workbench.reduceMotion": "on",
  "workbench.editor.defaultBinaryEditor": "default",
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.restoreViewState": false,
  "workbench.editor.sharedViewState": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "editor.tabSize": 2,
  "window.density.editorTabHeight": "compact",
  "problems.defaultViewMode": "table",
  "emmet.triggerExpansionOnTab": true,
  "workbench.colorTheme": "Everforest Dark",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "editor.fontLigatures": false,
  "terminal.integrated.fontWeight": "normal",
  "editor.fontFamily": "JetBrainsMono NF",
  "editor.fontWeight": "normal",
  "workbench.settings.applyToAllProfiles": [
      "editor.fontFamily"
  ],
  "editor.fontVariations": false,
  "glassit.alpha": 220,
  "window.menuBarVisibility": "toggle",
  "projectManager.git.baseFolders": [
      "$home/.ghq/github.com/iafshinafshin/dotfiles",
      "$home/.ghq/github.com/iafshinafshin/iafshin.github.io",
  ],
  "projectManager.any.baseFolders": [
      "$home/.ghq/github.com",
      "$home/.ghq/gitlab.com",
      "$home/.config",
  ],
  "projectManager.sortList": "Recent",
  // "editor.cursorStyle": "underline",
  "editor.codeActionsOnSave": {
      "source.addMissingImorts": "explicit",
      "source.organizeImports": "explicit"
  },
  "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
      "editor.defaultFormatter": "bradlc.vscode-tailwindcss", 
  },
  "[css]": {
      "editor.defaultFormatter": "bradlc.vscode-tailwindcss", 
  },

  // "[python]": {
  //     "editor.defaultFormatter": ["ms-python.black-formatter", "ms-python.vscode-pylance"],
  // },
  "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "editor.formatOnSave": true,
  "zenMode.centerLayout": false,
  "zenMode.hideLineNumbers": false,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.suggest.insertMode": "replace",
  "smoothtype.autoReload": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.stickyScroll.enabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "smoothtype.duration": 120,
  "editor.lineNumbers": "relative",
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(230, 97, 89, 0.7)",
  "vim.highlightedyank.textColor": "white",
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
      {
         "before":["<C-a>"],
         "after":["g", "g", "V", "G"],
      },
      {"before":["<tab>"],"commands":[":tabnext"]},
      {"before":["<S-tab>"],"commands":[":tabprev"]},
      {"before":["t", "e"],"commands":[":tabedit"]},
      {"before":["s", "h"],"after":["<C-w>", "h"]},
      {"before":["s", "l"],"after":["<C-w>", "l"]},
      {"before":["s", "j"],"after":["<C-w>", "j"]},
      {"before":["s", "k"],"after":["<C-w>", "k"]},
      {"before":["s", "s"],"commands":[":split"]},
      {"before":["s", "v"],"commands":[":vsplit"]},
      {"before":["K"],"commands":["editor.action.showHover"]},
      {"before":["<C-j>"],"commands":["editor.action.marker.next"]},
      {"before":["]", "d"],"commands":["editor.action.marker.next"]},
      {"before":["[", "d"],"commands":["editor.action.marker.perv"]},
      {"before":["<leader>", "c", "a"],"commands":["editor.action.quickFix"]},
      {"before":["<leader>", "p", "g"],"commands":["projectManager.listGitProjects#sideBarGit"]},
      {"before":["<leader>", "p", "a"],"commands":["projectManager.listAnyProjects#sideBarAny"]},
      {"before":["<leader>", "b"],"commands":["extension.toggleBool"]},
      {"before":["<Esc>"],"after":[":nohl"]},
      {"before":["<C-left>"],"after":["<C-w>", "<"]},
      {"before":["<C-right>"],"after":["<C-w>",">"]},
      {"before":["<C-up>"],"after":["<C-w>", "+"]},
      {"before":["<C-down>"],"after":["<C-w>", "-"]},
  ],
  "errorLens.fontFamily": "JetBrainsMono Nerd Font",
  // "workbench.editor.showTabs": "none",
}